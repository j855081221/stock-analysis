{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "lib": [
      "esnext",
      "dom"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noEmitHelpers": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules"
  ]
}